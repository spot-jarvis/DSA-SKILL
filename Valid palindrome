class Solution:
    def isPalindrome(self, s: str) -> bool:
        if len(s)==0:
            return True
        cleaned = ""
        for char in s:
            if char.isalnum():
                cleaned += char.lower()
        left ,right = 0,len(cleaned)-1
        while left < right:
            if cleaned[left] != cleaned[right]:
                return False
            left += 1
            right -= 1
        return True
